aluate(circuitCurrent, mA);¶   ¶    I?T[2] := I?T[2]+(90~degree);// Changing Phase angles manually¶    V?P := V?S[1] ? Z?T[2];//Not used in this problem¶    ¶    X?L := Evaluate(f10?14, ?) ? 90~degree;¶    lCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?P, 'R?OL <- X?L]);¶    I?L := Evaluate(lCurrent, mA);¶    ¶    R?R := 750~? ? 0~degree;¶    rCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?R, 'V?OL <- 'V?P, 'R?OL <- R?R]);¶    I?R := Evaluate(rCurrent, mA);¶    ¶    P?APP := Evaluate(f11?7, VA);¶¶}

