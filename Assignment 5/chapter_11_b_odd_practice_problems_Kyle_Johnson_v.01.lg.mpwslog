I?T[2] := 0~degree;¶    ¶    R?1 := 330~? ? 0~degree;  ¶    voltageParallel := Substitute2(ohmsLawA,['V?P <- 'V?P, 'I?T <- 'I?T, 'Z?P <- 'Z?P]);¶    V?P := Evaluate(voltageParallel, V);¶    ¶    voltageResistor := Substitute2(ohmsLawA,['V?P <- 'V?R1, 'I?T <- 'I?T, 'Z?P <- 'R?1]);¶    V?R1 := Evaluate(voltageResistor, V);¶    ¶    X?L := Evaluate(f10?14, ?) ? 90~degree;¶    lCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?P, 'R?OL <- X?L]);¶    I?L := Evaluate(lCurrent, mA);¶    ¶    R?R := 1.8~k? ? 0~degree;¶    rCurrentTwo := Substitute2(ohmsLaw,['I?OL <- 'I?R2, 'V?OL <- 'V?P, 'R?OL <- R?R]);¶    I?R2 := Evaluate(rCurrentTwo, mA);   ¶    I?T := Evaluate(circuitCurrent, mA);¶    ? := Evaluate(f11?22, degree);¶}¶¶/*¶-------------------------------------------------------------------------------------¶Answer:¶¶X?L = 1.89k?, Z?P = 1.3k?, ? = 43.6 ~degrees, R?S = 941?, X?S = 897?, R?T = 1271?,¶Z?T = 1556? ? 35.2 ~degrees, I?T = 6.43mA, V?S ? 35.2 ~degrees, V?R1 = 2.12V,¶V?P = 8.36V ? 43.6 ~degrees, I?L = 4.42mA ? -46.4 ~degrees, &¶I?R2 = 4.64mA ? 43.6 ~degrees¶-------------------------------------------------------------------------------------¶*/

