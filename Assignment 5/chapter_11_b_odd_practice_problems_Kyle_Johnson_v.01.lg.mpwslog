X?S1, 'Z?P <- 'Z?P1, '??unitless <- '??unitless1]);¶    X?S1 := Evaluate(xSubSOne);¶    //rewrite theta¶    thetaTwo := Substitute2(f11?5,['? <- '?1, 'R?T <- 'R?S1, 'X?L <- 'X?S1]);¶    ?1 := Evaluate(thetaTwo); ¶    //may have to rewrite theta¶    zSubOneOne := Substitute2(f11?4,['Z?T <- 'Z?T1, 'X?S <- 'X?S1, 'R?T <- 'R?S1]);//¶    zSubOneTwo := Substitute2(f11?5,['? <- '?1, 'X?L <- 'X?L1, 'R?T <- 'R?S1]);¶    Z?T1 := Evaluate(zSubOneOne, ?) ? Evaluate(zSubOneTwo, degree);  ¶    currentTotalOne := Substitute2(ohmsLaw,['I?OL <- 'I?T1, 'V?OL <- 'V?S1, 'R?OL <- Z?T1]);¶    I?T1 := Evaluate(currentTotalOne, mA); ¶    ¶//============================================================================================  ¶    V?S2 := 3~V ? 0~degree;¶    f?f2 := 2.5~kHz;¶    L?L2 := 10~mH;¶    xSubLTwo := Substitute2(f10?14,['X?L <- 'X?L2, 'f?f <- 'f?f2, 'L?L <- 'L?L2]);¶    X?L2 := Evaluate(xSubLTwo, ?);¶    R?R2 := 180~?;¶    zSubPTwo := Substitute2(f11?17,['Z?P <- 'Z?P2, 'X?L <- 'X?L2, 'R?R <- 'R?R2]);¶    Z?P2 := Evaluate(zSubPTwo, ?);¶    thetaThree := Substitute2(f11?22,['? <- '?3, 'X?L <- 'X?L2, 'R?R <- 'R?R2]);¶    ?3 := Evaluate(thetaThree);¶    ¶    thetaUnitlessThree := Substitute2(f11?22a,['??unitless <- '??unitless3, 'X?L <- 'X?L2, 'R?R <- 'R?R2]);¶    ??unitless3 := Evaluate(thetaUnitlessThree);¶    rSubSTwo := Substitute2(elevenTwentyThreeA,['R?S <- 'R?S2, 'Z?P <- 'Z?P2, '??unitless <- '??unitless3]);¶    R?S2 := Evaluate(rSubSTwo);¶    xSubSTwo :=Substitute2(elevenTwentyFourA,['X?S <- 'X?S2, 'Z?P <- 'Z?P2, '??unitless <- '??unitless3]);¶    X?S2 := Evaluate(xSubSTwo);¶    //rewrite theta¶    thetaThree := Substitute2(f11?5,['? <- '?2, 'R?T <- 'R?S2, 'X?L <- 'X?S2]);¶    ?2 := Evaluate(thetaThree); ¶    //may have to rewrite theta¶    zSubTwoOne := Substitute2(f11?4,['Z?T <- 'Z?T2, 'X?S <- 'X?S2, 'R?T <- 'R?S2]);//¶    zSubTwoTwo := Substitute2(f11?5,['? <- '?2, 'X?L <- 'X?L2, 'R?T <- 'R?S2]);¶    Z?T2 := Evaluate(zSubTwoOne, ?) ? Evaluate(zSubTwoTwo, degree);  ¶    currentTotalTwo := Substitute2(ohmsLaw,['I?OL <- 'I?T2, 'V?OL <- 'V?S2, 'R?OL <- Z?T2]);¶    I?T2 := Evaluate(currentTotalTwo, mA); ¶}¶¶/*¶-------------------------------------------------------------------------------------¶Answer:¶¶When f = 1 kHz: I?T = 50.6 mA ? -70.8 ~degrees, Z?T = 59.3? ? 70.8 ~degrees.¶When f = 2.5 kHz: I?T = 25.4 mA ? -48.9 ~degrees, Z?T = 118? ? 48.9 ~degrees¶-------------------------------------------------------------------------------------¶*/

