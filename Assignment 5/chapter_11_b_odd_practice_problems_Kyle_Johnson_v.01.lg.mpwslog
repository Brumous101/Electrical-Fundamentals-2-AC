ce¶    R?R := 750~?;¶    R?1 := 0~?;     ¶    Z?P := Evaluate(f11?17, ?);¶    ¶//-----------------Gets weird------------------¶    ? := Evaluate(f11?22a);¶    R?S := Evaluate(elevenTwentyThreeA);¶    X?S := Evaluate(elevenTwentyFourA);  ¶    R?T := Evaluate(f4?1, ?); ¶    Z?T := Evaluate(f11?4, ?) ? Evaluate(f11?5, degree);¶    ¶    circuitCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?S, 'R?OL <- Z?T]);¶    I?T := Evaluate(circuitCurrent, mA);¶    //kyle check notes I?T conflict kinda¶}¶¶/*¶-------------------------------------------------------------------------------------¶Answer:¶¶X?L = 177?, I?L = 678mA, I?R = 160mA, I?T = 697mA, ? = -76.7 ~degrees, & P?APP = 83.6 VA.¶-------------------------------------------------------------------------------------¶*/

