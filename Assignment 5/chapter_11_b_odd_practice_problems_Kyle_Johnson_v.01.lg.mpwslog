¶/*¶Symbols:¶¶? ? ? ?¶*/¶¶/*¶Problem 25.)¶¶Calculate the component current and power values for the circuit in Figure 11.42a.¶Then, calculate the values of I?T and P?APP.¶*/¶¶ProblemSolution([X?L, I?L, I?R, I?T, ?, P?APP])¶{¶    Formulas()¶    {¶        ohmsLaw := Formula( I?OL == V?OL/R?OL, "(3.1)", "78"); //Z?T can be subbed¶        f4?1 := Formula(R?T == R?1 + R?R + R?S, "4.1", "99"); //In series resistance total¶        f10?14 := Formula( X?L == 2 * Pi *f?f * L?L, "(10.14)", "302");¶        f11?4 := Formula( Z?T == Sqrt((X?S^2) + (R?T^2)), "(11.4)", "332");¶        f11?5 := Formula( ? == ArcTan(X?L/R?T) ~ rad, "(11.5)", "332");¶        f11?7 := Formula(P?APP == (V?S)*(I?T), "11.7", "339");¶        f11?17 := Formula(Z?P == ((X?L*R?R)/(Sqrt(X?L^2+R?R^2)) ), "11.17", "346");¶        f11?22 := Formula(? == ArcTan(R?R/X?L)~rad, "11.22", "348");¶        ¶//I had to make the formula 11?22 in degrees to a unitless number¶//I tried a lot of different ways to cancel and I could only get¶//it to cancel in radians and then convert it to a degree¶//value but without a unit. Cos() won't accept a degree unit.¶¶        f11?22a := Formula(??unitless == ((ArcTan(R?R/X?L)~rad)/(1~rad)) * 57.2958, "11.22", "348");//unitless¶        elevenTwentyThreeA := Formula(R?S == Z?P * Cos(??unitless), "11.23a", "354");¶        elevenTwentyFourA := Formula(X?S == Z?P * Sin(??unitless), "11.24a", "354");¶        ohmsLawA := Formula( V?P == I?T * Z?P, "(3.1)", "78,"); ¶    }¶    //X?L¶    V?S := 120~V ? 0~degree;¶    f?f := 60~Hz;¶    L?L := 470~mH;¶    X?L := Evaluate(f10?14, ?);¶    ¶    //impedance¶    R?R := 750~?;¶    R?1 := 0~?;     ¶    Z?P := Evaluate(f11?17, ?);¶    ¶//-----------------Gets weird------------------¶    ? := Evaluate(f11?22);¶    ??unitless := Evaluate(f11?22a);¶    R?S := Evaluate(elevenTwentyThreeA);¶    X?S := Evaluate(elevenTwentyFourA);  //¶    ¶    R?R := 0~?;//All resistors are now in series and combin|ed into one¶    R?T := Evaluate(f4?1, ?);¶    ¶    ? := Evaluate(f11?5); ¶    Z?T := Evaluate(f11?4, ?) ? Evaluate(f11?5, degree);¶    ¶    circuitCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?S, 'R?OL <- Z?T]);¶    I?T := Evaluate(circuitCurrent, mA);¶   ¶    I?T[2] := I?T[2]+(90~degree);// Changing Phase angles manually¶    V?P := V?S[1] ? Z?T[2];//Not used in this problem¶    ¶    X?L := Evaluate(f10?14, ?) ? 90~degree;¶    lCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?P, 'R?OL <- X?L]);¶    I?L := Evaluate(lCurrent, mA);¶    ¶    R?R := 750~? ? 0~degree;¶    rCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?R, 'V?OL <- 'V?P, 'R?OL <- R?R]);¶    I?R := Evaluate(rCurrent, mA);¶    ¶    P?APP := Evaluate(f11?7, VA);¶¶}¶¶/*¶-------------------------------------------------------------------------------------¶Answer:¶¶X?L = 177?, I?L = 678mA, I?R = 160mA, I?T = 697mA, ? = -76.7 ~degrees, & P?APP = 83.6 VA.¶-------------------------------------------------------------------------------------¶*/

