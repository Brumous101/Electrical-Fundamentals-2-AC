11?22a,['X?L <- 'X?LT]);¶    ??radian := Evaluate(phaseSub2);¶    ¶    R?S := Evaluate(elevenTwentyThreeA, );¶    X?S := Evaluate(elevenTwentyFourA);¶    ¶   ¶    R?R := 0~?;//All resistors are now in series and combin|ed into one¶    R?T := Evaluate(f4?1, ?);¶    ¶    X?L := X?LT;¶    L?L := L?T;¶    ¶    ? := Evaluate(f11?5); ¶    ¶    Z?T := Evaluate(f11?4, ?) ? Evaluate(f11?5, degree);¶    ¶    circuitCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?S, 'R?OL <- 'Z?P]);¶    I?T := Evaluate(circuitCurrent, mA);   ¶    V?S := 3~V ? 0~degree;¶    ¶    V?P := V?S[1] ? Z?T[2];¶    ¶    X?L := Evaluate(f10?14, ?) ? 90~degree;¶    X?L1 := Evaluate(xSubL1, ?) ? 90~degree;¶    X?L2 := Evaluate(xSubL2, ?) ? 90~degree;¶    ¶    lCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?L1, 'V?OL <- 'V?P, 'R?OL <- X?L1]);¶    I?L1 := Evaluate(lCurrent, mA);    ¶    ¶    l2Current := Substitute2(ohmsLaw,['I?OL <- 'I?L2, 'V?OL <- 'V?P, 'R?OL <- X?L2]);¶    I?L2 := Evaluate(l2Current, mA);¶    ¶    I?LT := (I?L1[1] + I?L2[1]) ? I?L1[2];¶    ¶    R?R := 510~? ? 0~degree;¶    rCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?R, 'V?OL <- 'V?P, 'R?OL <- R?R]);¶    I?R := Evaluate(rCurrent, mA);¶    ¶    ? := (-1 * Evaluate(phaseSub1, degree));¶}¶¶/*¶-------------------------------------------------------------------------------------¶Answer:¶¶X?L1 = 691?, X?L2 = 471?, I?L1 = 4.34 mA, I?L2 = 6.37mA, I?LT = 10.7mA, I?R=5.88mA,¶I?T = 12.2mA, & ? = -61.2 ~degrees¶-------------------------------------------------------------------------------------¶*/

