(elevenTwentyThreeA);¶    X?S := Evaluate(elevenTwentyFourA);  ¶    R?R := 0~?;//All resistors are now in series and combined into one¶    R?T := Evaluate(f4?1, ?); ¶    Z?T := Evaluate(f11?4, ?) ? Evaluate(f11?5, degree);¶    circuitCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?S, 'R?OL <- Z?T]);¶    I?T := Evaluate(circuitCurrent, mA);¶    //I?T[2] := I?T[2]+(90~degree);// Changing Phase angles manually¶    V?P := V?S[1] ? Z?T[2];//Not used in this problem¶    ¶    X?L := Evaluate(f10?14, ?) ? 90~degree;¶    lCurrent := Substitute2(ohmsLaw,['I?OL <- 'I?T, 'V?OL <- 'V?P, 'R?OL <- X?L]);¶    I?L := Evaluate(lCurrent, mA);¶}¶¶/*¶-------------------------------------------------------------------------------------¶Answer:¶¶X?L = 177?, I?L = 678mA, I?R = 160mA, I?T = 697mA, ? = -76.7 ~degrees, & P?APP = 83.6 VA.¶-------------------------------------------------------------------------------------¶*/

