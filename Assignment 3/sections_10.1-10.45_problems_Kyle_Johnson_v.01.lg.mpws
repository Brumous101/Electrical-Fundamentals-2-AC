steps[4] := Substitute('k, .25) steps[3]; 

steps[5] := Substitute('L?1, 10~mH) steps[4]; 

steps[6] := Substitute('L?2, 470~μH) steps[5]; 

steps[7] := NM(Eval(Substitute('(==), '(:=)) steps[6]) # mH);

TableForm(steps);

