    I?T := EvaluateFormula(f3?1a, μA);
    
    V?C := EvaluateFormula(f3?1b, V);
    
    V?R := EvaluateFormula(f3?1c, V);
    
    ϴ?V := EvaluateFormula(f13?5, deg);
    
    P?X := EvaluateFormula(?400, mVA);
    
    P?R := EvaluateFormula(f3?5, mW);
    
    P?APP := EvaluateFormula(f13?7);
    
    ϴ?P?APP := EvaluateFormula(f13?8, degree);
    
}
