ProblemSolution([Z?T, I?T, V?C, V?R, ϴ?V, P?R, P?X, P?APP, ϴ?P?APP])
{
    Formulas()
    {
        f12?11 := Formula(X?C~Ω == 1/(2*Pi*f?f~Hz*C?C~F), Label:"12.11", Page:"377");
        f13?4 := Formula(Z?T~Ω == Sqrt(X?C~Ω^2 + R?R~Ω^2), Label:"13.4", Page:"393");
        f3?1 := Formula(I?OL~A == V?OL~V/R?OL~Ω, Label:"3.1", Page:"78");
        f3?1a := Formula(f3?1, Label:"a", Page:"78", Substitutions:['I?OL <- 'I?T, 'V?OL <- 'V?S, 'R?OL <- 'Z?T]);
        f3?1b := Formula(f3?1, Label:"b", Page:"78", Substitutions:['I?OL <- 'I?T, 'V?OL <- 'V?C, 'R?OL <- 'X?C], Subject: 'V?C);
        f3?1c := Formula(f3?1, Label:"c", Page:"78", Substitutions:['I?OL <- 'I?T, 'V?OL <- 'V?R, 'R?OL <- 'R?R], Subject: 'V?R);
        f13?5 := Formula(ϴ~rad == ArcTan((-X?C)~Ω/R?R~Ω), Label:"13.5", Page:"393", Substitutions:['ϴ <- 'ϴ?V]);
        ?400 := Formula(P?X == I?I^2 * X?C, Page:"400", Substitutions:['I?I <- 'I?T]);
        f3?5 := Formula(P?P~W == I?I~A^2*R?R~Ω, Label:"3.5", Page:"339,85", Substitutions: ['P?P <- 'P?R, 'I?I <- 'I?T]);
        f13?7 := Formula(P?APP~VA == Sqrt(P?X~W^2 + P?R~W^2), Label:"13.7", Page:"401");
        f13?8 := Formula(ϴ~rad == ArcTan((-P?X)~W/P?R~W), Label:"13.8", Page:"402", Substitutions: ['ϴ <- 'ϴ?P?APP]);
    }
    C?C := 51~nF;
    f?f := 500~Hz;
    X?C := EvaluateFormula(f12?11, Ω);
    
    R?R := 12~kΩ;
    Z?T := EvaluateFormula(f13?4, kΩ);
    
