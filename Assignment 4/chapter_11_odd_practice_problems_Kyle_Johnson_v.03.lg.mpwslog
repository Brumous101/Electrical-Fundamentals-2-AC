/*¶Problem 3.5)¶¶Calculate the values of Z?T, I?T, V?L, V?R, and ? for the circuit shown in Figure 11.36a.¶*/¶¶ProblemSolution([Z?T, theta, I?T, V?L, V?R])¶{¶    // X?L¶    tenPointOneFour := Formula( X?L == 2 * Pi * f?f * L?L,"(10.4)","302");¶    ¶    f?f := 9~kHz;¶    ¶    L?L := 51~mH;¶    ¶    Evaluate(tenPointOneFour);¶    ¶    //impedance¶    elevenPointFour := Formula(Z?T == Sqrt(X?L^2 + R?R^2),"(11.4)","332");¶    ¶    R?R := 2.7~k?¶    ¶    elevenPointTwo := Formula(theta == ArcTan(X?L/R?R) ~rad,"(11.2)","330");¶    ¶    impedance := Evaluate(elevenPointFour, k?) ? Evaluate(elevenPointTwo, degree);¶    ¶    //I?T¶    ohmsLaw := Formula(I?OL == (V?OL/R?OL),"(3.1)","78");¶    ¶    circuitCurrent := Substitute2(ohmsLaw, ['I?OL <- 'I?T, 'V?OL <- 'V?S, 'R?OL <- impedance]);¶    ¶    V?S := 3~V ? 0~degree;¶    ¶    Evaluate(circuitCurrent, ?);¶    ¶    //V?L¶    elevenPointSix := Formula(V?n == V?S * (Z?n / Z?T),"(11.6)","334");¶    ¶    inductorVoltage := Substitute2(elevenPointSix, ['V?n <- 'V?L, 'Z?n <- X?L ? 90~degree, 'Z?T <- impedance]);¶    ¶    Evaluate(inductorVoltage);¶    ¶    //V?R¶    inductorVoltage := Substitute2(elevenPointSix, ['V?n <- 'V?R, 'Z?n <- R?R ? 0~degree, 'Z?T <- impedance]);¶    ¶    Evaluate(indductorVoltage);¶    ¶}¶¶/*¶-------------------------------------------------------------------------------------¶Answer:¶Z?T = 3.95k?, ? = 46.8°, I?T = 760?A, V?L = 2.19V, V?R = 2.05V ¶¶-------------------------------------------------------------------------------------¶*/

